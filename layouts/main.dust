

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">

  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  {+head /}
  <!-- Standard Favicon--> 
  <link rel="shortcut icon" href="images/favicon/favicon.ico">
  <link href="/css/main.min.css" rel="stylesheet">
  <link href='http://fonts.googleapis.com/css?family=Raleway:400,300,700,900%7COpen+Sans:400,300,700,800%7CMontserrat:400,700%7CCrimson+Text:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
</head>


<body>


  <!-- main navigation trigger -->
  <div class="main-nav visible-lg white-bg">
    <div class="mobile-toggle">
      <span></span>
      <span></span>
      <span></span>
    </div>
  </div>

  <header class="masthead visible-lg">
    <a href="/"><img class="master-logo" alt="" title="" src="/images/logo-black.png"/></a>


  </header>



  <!-- main menu : starts -->
  <section class="menu-panel fullheight"  style="background-image:url({shared.navigationImage});">

    <div class="row">


      <article class="col-md-6 fullheight">

        <div class="valign text-center slogan-holder">
          <div class="nav-logo-wrap">
            <a href="index.html"><img alt="" title="" src="/images/logo-transparent.png"/></a>
            <div class="slogan-wrap">
              <h3 class="white font3 slogan-text">Line Langballe</h3>
            </div>
          </div>
        </div>
      </article>



      <article class="col-md-6 fullheight nav-list-holder white-bg">
        <div class="valign">
          <nav class="nav-item-wrap">
            <ul class="main-nav-menu main-nav-menu-effect">

              <li class="trigger-sub-nav">
                <a class="main-nav-link" href="/index.html">home</a>               
              </li>

              <li class="trigger-sub-nav">
                <a class="main-nav-link" href="/projects/index.html">showcase</a>
              </li>    

            </ul>
          </nav>

          <div class="copy-credits">
            <ul class="nav-social">
              <li><a href="{shared.twitterUrl}"><img src="/images/social/01.svg" title="" alt=""></a></li>
              <li><a href="{shared.behanceUrl}"><img src="/images/social/04.svg" title="" alt=""></a></li>
              <li><a href="{shared.linkedinUrl}"><img src="/images/social/05.svg" title="" alt=""></a></li>
              <li><a href="mailto:{shared.email}"><img src="/images/mail.svg" title="" alt="" data-toggle="modal" data-target="#myModal"></a></li>
            </ul>
          </div>
        </div>
      </article>

    </div>
  </section>
  <!-- main menu : ends -->

  <!-- mobile only navigation : starts -->
  <nav class="mobile-nav hidden-lg">
    <ul class="slimmenu">
      <li>
        <a  href="/index.html">home</a>                 
      </li> 
      <li>
        <a  href="/projects/index.html">showcase</a>                 
      </li>  

    </ul>
  </nav>
  <!-- mobile only navigation : ends -->


  <!-- Master Wrap : starts -->

  <section class="mastwrap">
    {+body /}
    <footer id="mastfoot" class="mastfoot offwhite-bg">


      <!-- inner-section:starts -->
      <section class="inner-section social-wrap">
        <div class="container-fluid">
          <div class="row">
            <article class="col-md-5 text-left">
              <ul class="foot-social">
               <li><a href="{shared.twitterUrl}"><img src="/images/social/01.svg" title="" alt=""></a></li>
               <li><a href="{shared.behanceUrl}"><img src="/images/social/04.svg" title="" alt=""></a></li>
               <li><a href="{shared.linkedinUrl}"><img src="/images/social/05.svg" title="" alt=""></a></li>
               <li><a href="mailto:{shared.email}"><img src="/images/mail.svg" title="" alt="" data-toggle="modal" data-target="#myModal"></a></li>
             </ul>
           </article>
           <article class="col-md-6 text-right foot-logo">
            <p class="credits font2 dark">Line Langballe.<br/>Copyright &copy; 2014</p>
          </article>
          <article class="col-md-1 text-right foot-logo">
            <a href="index.html"><img alt="" title="" src="/images/logo-black.png"/></a>
          </article>
        </div>
      </div>
    </section>
    <!-- inner-section:ends -->

  </footer>
  <!-- Footer : ends -->
  
  <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
          <h4 class="modal-title" id="myModalLabel">Contact me</h4>
        </div>
        <div class="modal-body">
         <form action="" method="post" id="mycontactform" >
          <label for="name">Name:</label><br />
          <input type="text" name="name" id="name" /><br />
          <label for="email">Email:</label><br />
          <input type="text" name="email" id="email" /><br />
          <label for="message">Message:</label><br />
          <textarea name="message" id="message"></textarea><br />
          <input type="text" name="ups" id="ups" style="display:none"/><br />

          <div id="success" style="color:red;"></div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" id="submit">Send</button>
      </div>
    </div>
  </div>
</div>


<!-- Core JS Libraries -->
<script src="/scripts/main.min.js" type="text/javascript"></script>
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-57170956-1', 'auto');
  ga('send', 'pageview');

</script>
</body>
</html>